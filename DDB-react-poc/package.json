        "react-custom-properties": "^1.2.0",
        "react-dom": "^16.7.0",
        "react-draft-wysiwyg": "^1.13.2",
        "react-draggable": "^3.1.1",
        "react-grid-layout": "^0.16.6",
        "react-loadable": "^5.5.0",
        "react-modal": "^3.8.1",
        "react-redux": "^6.0.0",
        "react-resizable": "^1.7.5",
        "react-resize-detector": "^4.2.0",
        "react-scripts": "2.1.3",
        "react-sizeme": "^2.6.7",
