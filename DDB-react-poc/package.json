    "scripts": {
        "start": "react-scripts start",
        "build-react": "react-scripts build",
        "build": "bash -c 'export TARGET_DIR=\"./build\"; bash ./build.sh'",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "wpbuild": "npm run wpbuild:int",
        "wpcopy": "npm run wpcopy:int",
        "wpreset": "npm run wpreset:int",
        "wpbuild:int": "bash -c 'export TEMPLATE_DIR=\"AuditIntWorkflow\"; bash ./WP-build.sh'",
        "wpcopy:int": "bash -c 'export TEMPLATE_DIR=\"AuditIntWorkflow\"; bash ./WP-copy.sh'",
        "wpreset:int": "bash -c 'export TEMPLATE_DIR=\"AuditIntWorkflow\"; bash ./WP-reset.sh'",
        "wpbuild:us": "bash -c 'export TEMPLATE_DIR=\"AuditUSWorkflow\"; bash ./WP-build.sh'",
        "wpcopy:us": "bash -c 'export TEMPLATE_DIR=\"AuditUSWorkflow\"; bash ./WP-copy.sh'",
        "wpreset:us": "bash -c 'export TEMPLATE_DIR=\"AuditUSWorkflow\"; bash ./WP-reset.sh'",
        "buildtime:total": "bash ./buildTimes.sh",
        "parse:themes": "node ./tools/themeParser.js",
        "linecount:all": "bash -c \"git ls-files --directory . -- ':!:*png|' ':!:package-lock.json' ':!:package.json' | xargs wc -l; exit 0\"",
        "linecount:code": "bash -c \"git ls-files --directory src -- ':!:*png|' ':!:package-lock.json' ':!:package.json' | xargs wc -l; exit 0\"",
        "linecount:docs": "bash -c \"git ls-files --directory docs -- ':!:*png|' ':!:package-lock.json' ':!:package.json' | xargs wc -l; exit 0\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "vuepress": "^0.14.11"
    }
}
